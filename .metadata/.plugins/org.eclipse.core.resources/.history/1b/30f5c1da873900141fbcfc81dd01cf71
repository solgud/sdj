import java.util.Scanner;

public class TicTacToe
{

   public static void main(String[] args)
   {
      char[][] board = new char[3][3];
      
      System.out.printf(" %c | X | O %n", 'O');
      System.out.println("-----------");
      System.out.printf(" X | X | O %n");
      System.out.println("-----------");
      System.out.printf(" X | O | O %n");

   }

}
