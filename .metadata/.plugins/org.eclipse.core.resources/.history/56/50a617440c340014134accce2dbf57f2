public class FizzBuzz
{
   public static void main(String[] args)
   {
      int i = 1;
      String print;
      while (i <= 100)
      {
         if (i % 15 == 0)
            print = "FizzBuzz";
         else if (i % 3 == 0)
            print = "Fizz";
         else if (i % 5 == 0)
            print = "Buzz";
         else
            print = ""+i;
         System.out.println(print);
         i++;
      }
   }
}
