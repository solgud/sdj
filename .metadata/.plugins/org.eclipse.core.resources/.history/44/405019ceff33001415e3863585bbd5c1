
public class ModulusLoop
{

   public static void main(String[] args)
   {
      int i = 0;
      while( i < 400)
      {
         if (i % 2 == 0)
         {
            System.out.println("Even");
         }
         else
         {
            System.out.println("Odd");
         }
         i = i * 3;
      }
   }

}
