public class FizzBuzz
{

   public static void main(String[] args)
   {
      int i = 1;
      String print;
      
      while (i <= 100)
      {
         print = i % 15 ? "FizzBuzz" : i % 3 ? "Fizz" ? i % 5 ? "Buzz" : "" + i;

         if (i % 3 == 0)
         {
            System.out.println("Fizz");
         }
         else if (i % 5 == 0)
         {
            System.out.println("Buzz");
         }
         else if (i % 15 == 0)
         {
            System.out.println("FizzBuzz");
         }
         else
         {
            System.out.println(i);
         }
         i++;
      }
   }

}
