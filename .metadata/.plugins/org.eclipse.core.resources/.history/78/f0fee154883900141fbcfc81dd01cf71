import java.util.Scanner;

public class TicTacToe
{

   public static void main(String[] args)
   {
      char[][] board = new char[3][3];
      
      System.out.printf(" %c | %c | %c %n", board[0][0], board[0][1], board[0][2]);
      System.out.println("-----------");
      System.out.printf(" %c | %c | %c %n", ' ', ' ', ' ');
      System.out.println("-----------");
      System.out.printf(" %c | %c | %c %n", ' ', ' ', ' ');

   }

}
