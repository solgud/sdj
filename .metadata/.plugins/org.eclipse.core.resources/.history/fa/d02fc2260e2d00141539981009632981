import lejos.nxt.MotorPort;


public class RunEngine
{

   public static void main(String[] args) throws InterruptedException
   {
      // TODO Auto-generated method stub
      MotorPort.A.controlMotor(100, 1);
      Thread.sleep(1000);
      MotorPort.A.controlMotor(100, 1);
      
      MotorPort.A.controlMotor(100, 1);

      MotorPort.A.controlMotor(100, 1);
   }

}
