import java.util.Scanner;

public class To_n_doubled
{

   public static void main(String[] args)
   {
      int i=0, n;
      Scanner key = new Scanner(System.in);

      // Get value of n
      System.out.println("Give me a number!");
      n = key.nextInt();
      
      while (i <= n)
      {
         System.out.println(i + ", ");
         i += 2;
      }
      
      
   }

}
