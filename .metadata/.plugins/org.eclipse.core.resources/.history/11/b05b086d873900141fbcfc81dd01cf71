import java.util.Scanner;

public class TicTacToe
{

   public static void main(String[] args)
   {
      char[][] board = new char[3][3];
      
      System.out.printf(" O | X | O ");
      System.out.println("-----------");
      System.out.printf(" X | X | O ");
      System.out.println("-----------");
      System.out.printf(" X | O | O ");

   }

}
